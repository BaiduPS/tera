# 指定各种路径，onebox中无需修改
--log_dir=../log
--tera_tabletnode_path_prefix=../data/
--tera_zk_lib_log_path=../log/zk.log
--logbufsecs=0
--v=6

# 指定使用本地文件系统
--tera_leveldb_env_type=local

# 指定使用fake_zk模式, 只能本机访问tera
--tera_coord_type=fake_zk
--tera_master_query_tabletnode_period=1000
--tera_local_addr=127.0.0.1

# sdk
--tera_sdk_write_timeout=20000
--tera_sdk_read_timeout=20000
--tera_sdk_scan_timeout=30000

# others
--online_schema_update_enabled=true
--tera_metric_http_server_enable=false
